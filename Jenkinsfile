pipeline{
  
  agent any
  
  stages {
  
    satge("build") {
    
      steps {
        echo 'Building the app'
      }
    }
    
    satge("test") {
    
      steps {
        echo 'Testing the app'
      }
    }
    
    satge("deploy") {
    
      steps {
        echo 'Deploying the app'
      }
    }
  }
}
